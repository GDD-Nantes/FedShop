PREFIX bsbm-inst: <http://www4.wiwiss.fu-berlin.de/bizer/bsbm/v01/instances/>
PREFIX bsbm: <http://www4.wiwiss.fu-berlin.de/bizer/bsbm/v01/vocabulary/>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>

SELECT ?vendorId ?localOffer (GROUP_CONCAT(?product, "|") as ?groupProduct) 
WHERE {
    ?localOffer bsbm:product ?localProduct .
    BIND (REPLACE(STR(?localOffer), ".*(vendor)(\\d+).*", "$1$2") as ?vendorId ) 
    ?localProduct owl:sameAs ?product .
}
GROUP BY ?vendorId ?localOffer