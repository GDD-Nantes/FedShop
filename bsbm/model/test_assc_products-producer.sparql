# One product has one producer
# Product --(1..1)--> Producer
# pass if no ?nbProducerPerProduct != 1

PREFIX bsbm: <http://www4.wiwiss.fu-berlin.de/bizer/bsbm/v01/vocabulary/>

SELECT ?product (COUNT(DISTINCT ?producer) as ?nbProducerPerProduct)
WHERE {
    ?product bsbm:producer ?producer .
}
GROUP BY ?product
HAVING (COUNT(DISTINCT ?producer) != 1)